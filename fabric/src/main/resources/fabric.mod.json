{
    "schemaVersion": 1,
    "id": "noteblocklib",
    "version": "${version}",
    "name": "Note Block Library",
    "description": "Library for adding custom note block sounds.",
    "authors": [
        "ShaksterNano"
    ],
    "contact": {
        "homepage": "https://github.com/ShaksterNano/Note-Block-Library",
        "sources": "https://github.com/ShaksterNano/Note-Block-Library",
        "issues": "https://github.com/ShaksterNano/Note-Block-Library/issues"
    },
    "license": "MIT",
    "icon": "assets/noteblocklib/icon.png",
    "environment": "*",
    "entrypoints": {
        "mm:early_risers": [
            "io.github.shaksternano.noteblocklib.commonside.fabric.NoteBlockLibEarlyRiser"
        ]
    },
    "mixins": [
        "noteblocklib-common.mixins.json"
    ],
    "custom": {
        "modmenu": {
            "badges": [
                "library"
            ]
        },
        "notify_gradle_properties_url": "https://raw.githubusercontent.com/ShaksterNano/Note-Block-Library/1.18/gradle.properties",
        "notify_gradle_properties_key": "mod_version"
    },
    "depends": {
        "fabricloader": ">=0.14.0",
        "minecraft": ">=1.18"
    }
}
